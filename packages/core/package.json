{
  "name": "@gitpulse/core",
  "version": "0.1.0",
  "description": "Core analysis engine for GitPulse - AI-powered git contribution analyzer",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "rubrics"
  ],
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly --declaration --declarationDir dist",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "ai": "^6.0.78",
    "cosmiconfig": "^9.0.0",
    "p-limit": "^6.2.0",
    "simple-git": "^3.27.0",
    "zod": "^3.24.0"
  },
  "peerDependencies": {
    "@ai-sdk/anthropic": ">=1.0.0",
    "@ai-sdk/google": ">=1.0.0",
    "@ai-sdk/google-vertex": ">=4.0.0",
    "@ai-sdk/openai": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/openai": {
      "optional": true
    },
    "@ai-sdk/anthropic": {
      "optional": true
    },
    "@ai-sdk/google": {
      "optional": true
    },
    "@ai-sdk/google-vertex": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^3.0.40",
    "@ai-sdk/google": "^3.0.23",
    "@ai-sdk/google-vertex": "^4.0.0",
    "@ai-sdk/openai": "^3.0.26",
    "@types/node": "^25.2.2",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "license": "Apache-2.0"
}
